  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6",
    "outDir": "./app"
  },
  "exclude": [
    "typings/browser.d.ts",
    "typings/browser",
    "lib/libFileToExclude.d.ts",
    "node_modules",
    "wwwroot"
  ]
}
